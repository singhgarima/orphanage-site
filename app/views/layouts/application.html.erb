					<li><a href="home">Home</a></li>
					<li><a href="about-us">About Angel's Orphanage</a></li>
					<li><a href="how-you-can-help">How You Can Help</a></li>
					<li><a href="contact-us">Contact Us</a></li>
				</ul>
			</div>

			<div id="right-column">
				<%= yield :right_column %>
			</div>
			<div id="main">
				<%= yield %>
			</div>

			<div id="footer"> 
				This is the footer
			</div>
		</div>
